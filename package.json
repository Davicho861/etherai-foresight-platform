{
  "name": "praevisio-ai-landing",
  "private": true,
  "description": "Praevisio AI - Demo landing and interactive predictive dashboard (90% accuracy demo)",
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "server"
  ],
  "scripts": {
    "start": "concurrently \"npm run dev --workspace=server\" \"npm run dev\"",
    "start:native": "npx kill-port 4000 4001 4003 3000 3002 3003 4010 4020 && export NATIVE_DEV_MODE=true PORT=4003 VITE_PORT=3002 && concurrently \"npm run dev --workspace=server\" \"npm run dev\"",
    "start:native:test": "PORT=4003 VITE_PORT=3002 NATIVE_DEV_MODE=true concurrently \"npm run dev --workspace=server\" \"npm run dev\"",
    "native:setup": "npm ci && cd server && npm ci && npx prisma generate --schema=./prisma/schema.prisma && node seed.js",
    "dev": "vite",
    "new:feature": "node scripts/execute-directive.js --",
    "propose-plan": "node ./scripts/propose-plan.js --",
    "generate:component": "node ./scripts/generate-component.js",
    "consult:oracle": "node ./scripts/consult-oracle.js --",
    "create-report": "node ./scripts/create-report.js",
    "test": "jest",
    "build": "npx vite build",
    "build:dev": "npx vite build --mode development",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\" \"server/**/*.{js,ts}\" \"scripts/**/*.{js,ts}\" --max-warnings=200",
    "preview": "vite preview",
    "deploy:sovereign": "bash scripts/sovereign-ci.sh",
    "dev:container": "docker-compose up --build -d",
    "stop:container": "docker-compose down --volumes",
    "start:sdlc": "npm run start:native -- --open /sdlc-dashboard",
    "start:dashboard": "npm run start:native -- --open /dashboard",
    "start:demo": "npm run start:native -- --open /demo"
  },
  "dependencies": {
