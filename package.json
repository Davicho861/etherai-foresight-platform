{
  "name": "praevisio-ai-landing",
  "private": true,
  "description": "Praevisio AI - Demo landing and interactive predictive dashboard (90% accuracy demo)",
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "server"
  ],
  "scripts": {
    "start": "concurrently \"npm run dev --workspace=server\" \"npm run dev\"",
    "dev": "vite",
    "dev:container": "vite --host 0.0.0.0 --port 3002",
    "dev:live": "docker-compose up -d --build",
    "propose-plan": "node ./scripts/propose-plan.js --",
    "generate:component": "node ./scripts/generate-component.js",
    "generate:test": "node ./scripts/generate-e2e-test.js",
    "consult:oracle": "node ./scripts/consult-oracle.js --",
    "playwright": "playwright test",
    "test": "jest --passWithNoTests",
    "build": "npx vite build",
    "build:dev": "npx vite build --mode development",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\" \"server/**/*.{js,ts}\" \"scripts/**/*.{js,ts}\" \"playwright/**/*.{ts,js}\" --max-warnings=7",
    "preview": "vite preview"
  },
